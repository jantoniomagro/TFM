	Bdsk-Url-4 = {http://www.ivoa.net/Documents/TAP/20100225/PR-TAP-1.0-20100225.odt}}

@techreport{IVOA-Data-Access-Layer-Working-Group:2009lr,
	Abstract = {This document describes the Parameterized Query language (PQL). PQL has been developed for parameter-based table queries as part of the DAL Table Access Protocol (TAP). This document formalizes the syntax and meaning of PQL as a general parameter-based query language for querying tabular data. PQL complements the Astronomical Data Query Language (ADQL), with PQL providing a higher level, data model based query facility optimized for specific astronomical use cases, while ADQL provides a more expressive and powerful but lower level general query language based upon SQL.},
	Author = {{IVOA Data Access Layer Working Group}},
	Date-Added = {2013-09-05 20:32:50 +0000},
